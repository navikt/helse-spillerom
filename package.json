{
    "name": "spillerom",
    "version": "0.1.0",
    "private": true,
    "engines": {
        "node": "22"
    },
    "scripts": {
        "dev": "next dev --turbopack",
        "build": "next build",
        "start": "next start",
        "start:e2e": "NEXT_PUBLIC_IS_E2E=true NEXT_PUBLIC_BASE_PATH= NEXT_PUBLIC_ASSET_PREFIX= node .next/standalone/server.js | pino-pretty",
        "build:e2e": "NEXT_PUBLIC_IS_E2E=true NEXT_PUBLIC_BASE_PATH= NEXT_PUBLIC_ASSET_PREFIX= next build",
        "prettier:write": "prettier --write .",
        "prettier:check": "prettier --check .",
        "tsc": "tsc",
        "play": "npm run tsc && npx playwright test --ui",
        "play-headless": "npm run tsc && npx playwright test",
        "lint": "eslint --ext=ts,tsx src",
        "test": "vitest --config vitest.config.mts --watch=false",
        "lint:fix": "eslint --ext=ts,tsx src --fix",
        "format": "npm run prettier:write && npm run lint:fix"
    },
    "dependencies": {
        "@faker-js/faker": "^10.1.0",
        "@hookform/resolvers": "^5.2.2",
        "@navikt/aksel-icons": "^7.32.2",
        "@navikt/ds-css": "^7.32.3",
        "@navikt/ds-react": "^7.32.3",
        "@navikt/ds-tailwind": "^7.32.3",
        "@navikt/ds-tokens": "^7.32.3",
        "@navikt/next-api-proxy": "^4.1.5",
        "@navikt/next-logger": "^4.4.1",
        "@navikt/oasis": "^3.8.0",
        "@tanstack/react-query": "^5.90.5",
        "clsx": "^2.1.1",
        "dayjs": "^1.11.18",
        "motion": "^12.23.24",
        "next": "^16.0.1",
        "next-themes": "^0.4.6",
        "nextleton": "^0.6.1",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "react-hook-form": "^7.65.0",
        "remeda": "^2.32.0",
        "tailwind-merge": "^3.3.1",
        "uuid": "^13.0.0",
        "zod": "^4.1.12"
    },
    "devDependencies": {
        "@axe-core/playwright": "^4.10.2",
        "@navikt/eslint-config-teamsykmelding": "^8.1.1",
        "@playwright/test": "^1.56.1",
        "@tailwindcss/postcss": "4.1.14",
        "@types/node": "^22.17.0",
        "@types/react": "^19.2.2",
        "@types/react-dom": "^19.2.2",
        "autoprefixer": "^10.4.21",
        "eslint": "^8.57.1",
        "eslint-config-next": "^15.5.6",
        "playwright-core": "^1.54.2",
        "postcss": "^8.5.6",
        "postcss-import": "^16.1.1",
        "postcss-loader": "8.2.0",
        "prettier-plugin-tailwindcss": "^0.7.1",
        "tailwindcss": "4.1.14",
        "typescript": "^5.9.3",
        "vitest": "^3.2.4"
    },
    "eslintConfig": {
        "extends": [
            "@navikt/eslint-config-teamsykmelding",
            "next/core-web-vitals"
        ],
        "rules": {
            "@typescript-eslint/explicit-function-return-type": "off"
        }
    }
}
