{
    "name": "spillerom",
    "version": "0.1.0",
    "private": true,
    "engines": {
        "node": "24"
    },
    "scripts": {
        "dev": "next dev --turbopack",
        "lokal": "NEXT_PUBLIC_RUNTIME_ENV=lokal next dev --turbopack",
        "build": "next build",
        "start": "next start",
        "start:e2e": "NEXT_PUBLIC_IS_E2E=true NEXT_PUBLIC_BASE_PATH= NEXT_PUBLIC_ASSET_PREFIX= node .next/standalone/server.js | pino-pretty",
        "build:e2e": "NEXT_PUBLIC_IS_E2E=true NEXT_PUBLIC_BASE_PATH= NEXT_PUBLIC_ASSET_PREFIX= next build",
        "prettier:write": "prettier --write .",
        "prettier:check": "prettier --check .",
        "tsc": "tsc",
        "play": "npm run tsc && npx playwright test --ui",
        "play-headless": "npm run tsc && npx playwright test",
        "lint": "eslint --ext=ts,tsx src",
        "test": "vitest --config vitest.config.mts --watch=false",
        "lint:fix": "eslint --ext=ts,tsx src --fix",
        "format": "npm run prettier:write && npm run lint:fix"
    },
    "dependencies": {
        "@hookform/resolvers": "^5.2.2",
        "@navikt/aksel-icons": "^7.35.0",
        "@navikt/ds-css": "^7.35.0",
        "@navikt/ds-react": "^7.35.0",
        "@navikt/ds-tailwind": "^7.35.0",
        "@navikt/ds-tokens": "^7.35.0",
        "@navikt/next-api-proxy": "^5.0.0",
        "@navikt/next-logger": "^4.4.1",
        "@navikt/oasis": "^3.8.0",
        "@tanstack/react-query": "^5.90.11",
        "clsx": "^2.1.1",
        "dayjs": "^1.11.19",
        "motion": "^12.23.24",
        "next": "16.0.7",
        "next-themes": "^0.4.6",
        "react": "19.2.1",
        "react-dom": "19.2.1",
        "react-hook-form": "^7.68.0",
        "remeda": "^2.32.0",
        "tailwind-merge": "^3.4.0",
        "uuid": "^13.0.0",
        "zod": "^4.1.13"
    },
    "devDependencies": {
        "@axe-core/playwright": "^4.11.0",
        "@navikt/tsm-eslint-react": "^1.0.0",
        "@navikt/tsm-prettier": "^2.0.0",
        "@playwright/test": "^1.57.0",
        "@tailwindcss/postcss": "4.1.17",
        "@types/node": "^24.10.1",
        "@types/react": "19.2.7",
        "@types/react-dom": "19.2.3",
        "autoprefixer": "^10.4.22",
        "babel-plugin-react-compiler": "^1.0.0",
        "eslint": "^9.39.1",
        "eslint-config-next": "16.0.7",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.4",
        "playwright-core": "^1.57.0",
        "postcss": "^8.5.6",
        "postcss-import": "^16.1.1",
        "postcss-loader": "8.2.0",
        "prettier-plugin-tailwindcss": "^0.7.1",
        "tailwindcss": "4.1.17",
        "typescript": "^5.9.3",
        "vitest": "^4.0.15"
    },
    "prettier": "@navikt/tsm-prettier"
}
