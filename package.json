        "@tanstack/react-query": "^5.69.0",
        "@types/node": "20.14.10",
        "@types/react": "18.3.12",
        "@types/react-dom": "18.3.1",
        "autoprefixer": "10.4.21",
        "classnames": "^2.5.1",
        "dayjs": "^1.11.12",
        "motion": "^12.5.0",
        "next": "^15.2.4",
        "nextleton": "^0.6.1",
        "postcss": "8.5.3",
        "react": "18.3.1",
