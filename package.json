{
    "name": "spillerom",
    "version": "0.1.0",
    "private": true,
    "engines": {
        "node": "22"
    },
    "scripts": {
        "dev": "next dev --turbopack",
        "build": "next build",
        "start": "next start",
        "start:e2e": "NEXT_PUBLIC_IS_E2E=true NEXT_PUBLIC_BASE_PATH= NEXT_PUBLIC_ASSET_PREFIX= node .next/standalone/server.js | pino-pretty",
        "build:e2e": "NEXT_PUBLIC_IS_E2E=true NEXT_PUBLIC_BASE_PATH= NEXT_PUBLIC_ASSET_PREFIX= next build",
        "prettier:write": "prettier --write .",
        "prettier:check": "prettier --check .",
        "tsc": "tsc",
        "play": "npx playwright test --ui",
        "play-headless": "npx playwright test",
        "lint": "eslint --ext=ts,tsx src",
        "test": "vitest --config vitest.config.mts",
        "lint:fix": "eslint --ext=ts,tsx src --fix",
        "format": "npm run prettier:write && npm run lint:fix"
    },
    "dependencies": {
        "@faker-js/faker": "^9.9.0",
        "@hookform/resolvers": "^5.2.1",
        "@navikt/aksel-icons": "^7.26.0",
        "@navikt/ds-css": "^7.26.0",
        "@navikt/ds-react": "^7.26.0",
        "@navikt/ds-tailwind": "^7.26.0",
        "@navikt/ds-tokens": "^7.27.0",
        "@navikt/next-api-proxy": "^4.1.4",
        "@navikt/next-logger": "^4.1.5",
        "@navikt/oasis": "^3.8.0",
        "@tanstack/react-query": "^5.84.1",
        "clsx": "^2.1.1",
        "dayjs": "^1.11.13",
        "motion": "^12.23.12",
        "next": "^15.3.5",
        "next-themes": "^0.4.6",
        "nextleton": "^0.6.1",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "react-hook-form": "^7.62.0",
        "remeda": "^2.29.0",
        "tailwind-merge": "^3.3.1",
        "uuid": "^11.1.0",
        "zod": "^4.0.15"
    },
    "devDependencies": {
        "@axe-core/playwright": "^4.10.2",
        "@navikt/eslint-config-teamsykmelding": "^7.2.0",
        "@playwright/test": "^1.54.2",
        "@tailwindcss/postcss": "4.1.11",
        "@types/node": "^22.17.0",
        "@types/react": "^19.1.9",
        "@types/react-dom": "^19.1.7",
        "autoprefixer": "^10.4.21",
        "eslint": "^8.57.1",
        "eslint-config-next": "^15.4.6",
        "playwright-core": "^1.54.2",
        "postcss": "^8.5.6",
        "postcss-import": "^16.1.1",
        "postcss-loader": "8.1.1",
        "prettier-plugin-tailwindcss": "^0.6.14",
        "tailwindcss": "4.1.11",
        "typescript": "^5.9.2",
        "vitest": "^3.2.4"
    },
    "eslintConfig": {
        "extends": [
            "@navikt/eslint-config-teamsykmelding",
            "next/core-web-vitals"
        ],
        "rules": {
            "@typescript-eslint/explicit-function-return-type": "off"
        }
    }
}
